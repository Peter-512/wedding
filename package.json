{
	"name": "wedding",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint . && inlang lint --project project.inlang",
		"format": "prettier --write .",
		"translate": "for file in messages/*.json; do \n jq 'walk(if type == \"object\" then with_entries(select(.value != \"\")) else . end)' \"$file\" > temp && mv temp \"$file\" \n done && inlang machine translate --project project.inlang"
	},
	"devDependencies": {
		"@inlang/cli": "^2.18.1",
		"@inlang/paraglide-js": "1.11.2",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"@sveltejs/enhanced-img": "^0.3.3",
		"@sveltejs/kit": "^2.18.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/typography": "^0.5.14",
		"@types/eslint": "^9.6.0",
		"@types/node": "^22.2.0",
		"@types/nodemailer": "^6.4.15",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.3.5",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^3.0.2",
		"formsnap": "^2.0.0",
		"globals": "^15.0.0",
		"lucide-svelte": "^0.477.0",
		"prettier": "^3.1.1",
		"prettier-plugin-sort-imports": "^1.8.6",
		"prettier-plugin-sort-json": "^4.0.0",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.22.1",
		"svelte-check": "^4.1.4",
		"sveltekit-superforms": "^2.22.1",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vaul-svelte": "1.0.0-next.0",
		"vite": "^6.0.6",
		"vitest": "^2.0.0",
		"zod": "^3.24.1"
	},
	"type": "module",
	"dependencies": {
		"@inlang/paraglide-sveltekit": "0.11.0",
		"@mdi/js": "^7.4.47",
		"@supabase/supabase-js": "^2.48.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"svelte-ux": "^0.77.0",
		"tailwind-merge": "^2.5.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss-animate": "^1.0.7"
	},
	"pnpm": {
		"ignoredBuiltDependencies": [
			"@sveltejs/kit"
		],
		"onlyBuiltDependencies": [
			"@sveltejs/kit",
			"esbuild",
			"sharp",
			"svelte-preprocess"
		]
	}
}
